function wel(pard,pard2)
local weldd = Instance.new("Weld",pard)
local cfdiff = pard.CFrame:Inverse() * pard2.CFrame
weldd.Part0 = pard2
weldd.Part1 = pard
weldd.C1 = cfdiff
end
wel(rightup,rightmid)
wel(leftup,leftmid)
wel(hed,faic)
wel(tors,hed)
wel(tors,belle)
belle.Anchored = false
